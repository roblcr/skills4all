<form class="mt-1" method="post" action="{{ path('app_car_delete', {'id': car.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette voiture ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ car.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>
